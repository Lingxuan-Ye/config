[core]
	autocrlf = input
	safecrlf = warn
	ignorecase = false
	quotepath = false
	editor = code --wait
	symlinks = true

[init]
	defaultBranch = main

[user]
	name = Lingxuan Ye
	email = love@lingxuan.io

[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
